-- Variables para controlar la b칰squeda y el teletransporte
local isSearching = false
local isTeleporting = false -- Nuevo estado para el teletransporte
local teleportInterval = 40 -- Tiempo en segundos entre teletransportes

local function teleportToRandomObject(objectName)
    while isSearching do
        local objects = {}
        for _, obj in pairs(game.Workspace:GetChildren()) do
            if obj.Name == objectName then
                table.insert(objects, obj)
            end
        end

        if #objects > 0 then
            local randomObject = objects[math.random(1, #objects)]
            game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(randomObject.CFrame)
            wait(0.2)
        else
            break
        end
    end
end

local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()
local w = library:CreateWindow("UGC游땓SCRIPT")

local b = w:CreateFolder("RECOLECTAR ")
local a = w:CreateFolder("DETENER ")
local e = w:CreateFolder("ENTREGAR ")

b:Button("INICIAR", function()
    isSearching = true
    game:GetService("StarterGui"):SetCore("SendNotification", { 
        Title = "TELETRANSPORTE ACTIVADO",
        Text = "Iniciando teletransporte continuo para el objeto robot.",
        Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150"
    })
    teleportToRandomObject("robot")
end)

a:Button("INICIAR", function()
    isSearching = false -- Detener el teletransporte continuo
    isTeleporting = false -- Asegurarse de que el teletransporte a la zona de entrega se detenga
    game:GetService("StarterGui"):SetCore("SendNotification", { 
        Title = "TELETRANSPORTE DETENIDO",
        Text = "Has detenido el teletransporte continuo.",
        Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150"
    })
end)

e:Button("INICIAR", function()
    local player = game:GetService("Players").LocalPlayer
    local destination = game.Workspace:FindFirstChild("WithdrawalZone") -- ubicaci칩n

    if destination then
        isTeleporting = true -- Iniciar estado de teletransporte autom치tico
        game:GetService("StarterGui"):SetCore("SendNotification", { 
            Title = "TELETRANSPORTE ACTIVADO",
            Text = "Teletransportando autom치ticamente cada 40 segundos a la ubicaci칩n deseada.",
            Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150"
        })
        
        while isTeleporting do
            player.Character:SetPrimaryPartCFrame(destination.CFrame) -- Teletransporta al jugador
            wait(teleportInterval) -- Espera 40 segundos antes de teletransportar nuevamente
        end
    else
        game:GetService("StarterGui"):SetCore("SendNotification", { 
            Title = "ERROR",
            Text = "No se encontr칩 el destino.",
            Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150"
        })
    end
end)
